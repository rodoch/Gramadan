    <Version>0.0.1-alpha</Version>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <PackageId>Ansa.Gramadan.Standard</PackageId>
    <Company>Ansa.Gramadan.Standard</Company>
    <Product>Ansa.Gramadan.Standard</Product>
